# BLuxA Corp - Frontend Environment Variables
# Copy this file to .env.local in your React app and fill in your values

# API Configuration
REACT_APP_API_URL=https://your-cloud-run-service-url.run.app
REACT_APP_API_VERSION=v1

# Stripe Configuration (Public Keys Only)
REACT_APP_STRIPE_PUBLISHABLE_KEY=pk_test_your-stripe-publishable-key

# Supabase Configuration (Public Keys Only)
REACT_APP_SUPABASE_URL=https://your-project.supabase.co
REACT_APP_SUPABASE_ANON_KEY=your-supabase-anon-key

# Google Maps (if using address autocomplete)
REACT_APP_GOOGLE_MAPS_API_KEY=your-google-maps-api-key

# Application Configuration
REACT_APP_COMPANY_NAME=BLuxA Corp
REACT_APP_COMPANY_EMAIL=contact@bluxacorp.com
REACT_APP_COMPANY_PHONE=+1-555-BLUXA-01

# Development Configuration
REACT_APP_DEBUG=false
REACT_APP_LOG_LEVEL=error

# Vercel Configuration (if deploying to Vercel)
VERCEL_URL=your-vercel-app.vercel.app
